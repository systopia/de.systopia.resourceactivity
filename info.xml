<?xml version="1.0"?>
<extension key="de.systopia.resourceactivity" type="module">
  <file>resourceactivity</file>
  <name>de.systopia.resourceactivity</name>
  <description>CiviResource resource management coupled with CiviCRM activities.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.resourceactivity</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.resourceactivity</url>
    <url desc="Support">https://github.com/systopia/de.systopia.resourceactivity/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Resourceactivity</namespace>
    <format>22.10.0</format>
    <angularModule>crmResourceactivity</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
